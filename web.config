<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
	<!-- log4net Configuration -->
	<log4net>
		<root>
			<level value="ALL" />
			<appender-ref ref="RollingFile" />
		</root>
		<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
			<appendToFile value="true" />
			<file value="D:\MoxVideo\MoxVideo\logs\logs.txt" />
			<rollingStyle value="Date" />
			<staticLogFileName value="true" />
			<datePattern value="yyyyMMdd-HHmm.'txt'" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %-5level %logger.%method [%line] - MESSAGE: %message%newline" />
			</layout>
		</appender>
	</log4net>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\MoxVideo.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
    </system.webServer>
  </location>
</configuration>
